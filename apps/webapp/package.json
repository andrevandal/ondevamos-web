  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest --config ./vitest.config.ts",
    "test:coverage": "vitest run --config ./vitest.config.ts --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "drizzle:introspect": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/drizzle-kit/index.cjs introspect:mysql",
    "drizzle:generate": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/drizzle-kit/index.cjs generate:mysql --out server/migrations --schema server/schemas/db/**/*.ts",
    "drizzle:migrator": "cross-env NODE_ENV=migration vite-node server/utils/migrateToLatest.ts",
    "process:places": "cross-env NODE_ENV=migration vite-node server/utils/processPlacesCsv.ts",
    "process:places:w": "cross-env NODE_ENV=migration vite-node --watch server/utils/processPlacesCsv.ts",
    "drizzle:studio": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/drizzle-kit/index.cjs studio"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lenyeg/ondevamos-web.git"
  },
